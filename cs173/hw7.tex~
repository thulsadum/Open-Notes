\begin{enumerate}

\item (10 points)
  Consider the inequallity:
  \begin{sageblock}
    P(n) = 2^n > n^2 + n
  \end{sageblock}
  \begin{enumerate}
  \item {
      What is the smallest positive integer value of $n$ for which this inequality 
      holds?\\
      n=5 $\sage{P(5)}$ which is true \\
      Notice n is not less that 5 because if n = 4, $\sage{P(4)}$ which is false
    }
  \item (8 points) Prove that this inequality holds for integers $n$ greater than
    or equal to your lower bound from part (a).
  \end{enumerate}

  Claim: $P(n) = 2^n > n^2 + n$\\\\
  Base: 
  \begin{enumerate}
  \item Let $n = 5, n\in\mathbb{Z}$
  \item Observe that $2^5 = \sage{2^5} > \sage{2^2 + 2} = 2^2 + 2$
  \item Thus $P(5)$ is true
  \end{enumerate}

  Induction:
  \begin{enumerate}
  \item I.H: Suppose that for some integer $n$ $P(n)$ is true and $n >4$
  \item N.T.S:  $2^{n+1} > (n+1)^2 + n + 1 = n^2+3n+2 = (n+1)(n+2)$
  \item Notice that $2^{n+1} = 2*2^2$
  \item Using the I.H. $2*2^n > 2*(n^2+n)$
  \item Observe that $2*(n^2+n) = 2n(n+1)$
  \item Also Observe that $2n > (n+2)$ if $n>2$
  \item Thus $2^{n+1} > 2(n^2+n) = 2n(n+1) > (n+2)(n+1) = n^2+3n+2 = (n+1)^2 + (n+1)$
  \item Simplifying $2^{n+1} > (n+1)^2+(n+1)$
  \item $\square$

\newpage
%--------------------------------------------------------------------------------------

  \end{enumerate}
\item Prove that consecutive Fibonacci numbers are relatively prime ($\gcd{a,b}=`$). 
  \begin{quote}
      Def of Fibonacci numbers : $F(0) = 0, F(1) = 1, F(n+1) = F(n) + F(n-1)$
  \end{quote}
 
  Claim: $\gcd{(F(n),F(n+1))} = 1$\\\\
  Base:
  \begin{enumerate}
  \item Let $n=0$ for some integer $n$
  \item Notice that $F(n)= 0$ and $F(n+1) = 1$
  \item Also note the factorization of $0 = 0*1$ and $1 = 1*1$
  \item Because the only common prime is $1$ the $\gcd{F(0),F(1)} = \gcd{1,0} = 1$
  \item Let $n=1$, notice that $F(1)$ and $F(2)$ equal 1. Therefore the $gcd$ is obviously $1$
  \end{enumerate}

  Induction
  \begin{enumerate}
  \item I.H. Suppose for some integer $n$, $\gcd{(F(n),F(n+1))} = 1$ where $n = 0,1,2,..k$ $n,k\in\mathbb{Z}$
  \item N.T.S. $\gcd{F(k),F(k+1)} = 1$
  \item Applying the def of $F(n)$, $F(k+1) = F(k)+F(k-1)$
  \item Suppose $\gcd{F(k+1),F(k)} \neq 1,$
  \item Then $F(k+1) = F(k)*q, q\in\mathbb{Z}$
  \item Therefore $q = \frac{F(k+1)}{F(k)}$
  \item Substituting, $q = \frac{F(k)+F(k-1)}{F(k)} = 1 + \frac{F(k-1)}{F(k)}$
  \item Let $p = q - 1, p\in\mathbb{Z}$
  \item Observe then that $F(k-1) = p*F(k)$
  \item Applying the I.H. $F(k)$ and $F(k-1)$ are relatively prime and thus $p = 1$, $p=0$ or $p$ is not an integer
  \item Case 1: $p = 1$, then $F(k) = F(k-1)$, however the only Fibonacci numbers that are equal are $F(1)$ and $F(2)$, and the $gcd$ of $F(1)$ and $F(2)$ is $1$
  \item Case 2: $p = 0$, then $F(k-1)$ must be $0$ and $F(k)$ must be $1$, and the $\gcd{1,0} = 1$
  \item Case 3: $p$ is not an integer
  \item Observe that by closure, $p$ is also not an integer
  \item However for the $gcd \neq 1$, $p$ must be an integer. - contradiction
  \item Thus $F(k)$ and $F(k+1)$ must be relatively prime
  \item # 
    \end{enumerate}


