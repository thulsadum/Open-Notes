\item (10 points) Define $a=\frac{1+\sqrt{5}}{2}$,
  $b=\frac{1-\sqrt{5}}{2}$ and let F(n) be the $n^{th}$ Fibonacci number.

  \begin{enumerate}
  \item Show that $a^2=a+1$ and $b^2=b+1$
    \begin{enumerate}
    \item Let $a=\frac{1+\sqrt{5}}{2}$, and $b =\frac{1-\sqrt{5}}{2}$ $a,b\in\mathbb{Z}$
    \item Notice that $a^2 = \frac{(1+\sqrt{5})^2}{4}$ and $b^2 =\frac{(1-\sqrt{5})^2}{4}$
    \item Simplifying $a^2 = \frac{1+2\sqrt{5}+5)}{4} = \frac{2(3+\sqrt{5})}{4} = 1 + \frac{1 + \sqrt(5)}{2} = 1 + a$
    \item Similarly  $b^2 = \frac{1-2\sqrt{5}+5)}{4} = \frac{2(3-\sqrt{5})}{4} = 1 + \frac{1 - \sqrt(5)}{2} = 1 + b$
    \end{enumerate}
  \item Claim $F(n)=\frac{a^n-b^n}{a-b}$.
    \begin{enumerate}
    \item Bases: $n = 0,1$
    \item Observe that $F(0) = 0 = \frac{0}{a-b} =  \frac{a^0-b^0}{a-b}$.
    \item Also Obeserve that $F(1) = 1 = \frac{a-b}{a-b} = \frac{a^1-b^1}{a-b}$
    \end{enumerate}

    \begin{enumerate}
    \item I.H. Supposed that for some integer $n$, $n=0,1...k$, $F(n)=\frac{a^n-b^n}{a-b}$.
    \item N.T.S. $F(k+1)=\frac{a^{k+1}-b^{k+1}}{a-b}$.
    \item Applying the def of $F(n)$, $F(k+1)= F(k) + F(k-1)$
    \item Using the I.H. $F(k)=\frac{a^k-b^k}{a-b}$
    \item Similarly  $F(k-1)=\frac{a^{k-1}-b^{k-1}}{a-b}$
    \item This $F(k+1) = 
    \end{enumerate}
  \end{enumerate}

